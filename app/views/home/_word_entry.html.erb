  <tr id=<%= word_entry.id -%>>
      <td><%= word_entry.eng_word.downcase -%></td>
      <td><% if !word_entry.eng_desc.nil? %>
        <ul> <%= word_entry.format_eng_def -%> </ul>
        <% else %>
            <img src="images/spinner.gif" height="10" alt="loading" />
        <% end %>
    </td>
    <td>
     <%= link_to "Edit", :controller => "vocentries", :action => "edit", :id => word_entry.id %>
    </td>
  </tr>
