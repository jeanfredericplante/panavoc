<h1>Today's vocab</h1>

<table>
  <tr>
    <th>Eng word</th>
    <th>Eng desc</th>
    <th></th>
  </tr>
  <div id="wodz">
    <%= render(:partial => "word_entry",:collection => @vocentries) %>
  <select id="options" onchange="<%= remote_function(:update => "wodz", :url => {:controller => 'home', :action => 'get_definition', :collection => @vocentries}) %>">
      <option value="0">Hello</option>
      <option value="1">World</option>
    </select>
  </div>
</table>
